<?xml version="1.0" encoding="UTF-8"?>

<launch4jConfig>
  <headerType>gui</headerType>
  <jar>stendhal-starter.jar</jar>
  <outfile>build/lib/stendhal-starter-1.28.5.exe</outfile>
  <dontWrapJar>true</dontWrapJar>
  <priority>normal</priority>
  <stayAlive>false</stayAlive>
  <icon>icon.ico</icon>
  <jre>
    <!-- "path" may need to be set to point to JRE root folder if registry keys for Java
         are not set up correctly. Example:

             <path>C:\Program Files\Java\jdk-9</path>

         More info on registry key values can be found here:
             https://docs.oracle.com/javase/9/install/installation-jdk-and-jre-microsoft-windows-platforms.htm
    -->
    <path></path>
    <bundledJre64Bit>false</bundledJre64Bit>
    <minVersion>1.8.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64/32</runtimeBits>
  </jre>
</launch4jConfig>
